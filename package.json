{
  "name": "fullstackdev-challenge",
  "version": "1.0.0",
  "description": "Fullstack Dev Challenge",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:*",
    "start:client": "webpack serve --mode=development",
    "start:server": "NODE_ENV=development nodemon server/src/index.ts --exec babel-node --extensions .ts .js",
    "start:systems": "babel-node tools/mock_systems",
    "test": "jest test -c test/jest.config.js",
    "test:watch": "jest --watch test -c test/jest.config.js",
    "format": "prettier --write './**/*.{ts,tsx}'",
    "lint": "eslint . --ext .ts,.js "
  },
  "author": "Oxehealth",
  "nodemonConfig": {
    "watch": [
      "server/src",
      ".env"
    ],
    "ext": "js,ts,json",
    "ignore": [
      "server/src/**/*.test.ts"
    ],
    "exec": "babel-node --extensions .ts .js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "app-root-path": "^3.0.0",
    "chart.js": "^3.6.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "26.6.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "superagent": "^6.1.0",
    "winston": "^3.3.3",
    "ws": "^8.3.0",
    "zeromq": "^5.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.12.1",
    "@types/ws": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^15.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "^30.7.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^4.5.2",
    "jest": "26.6.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "react-refresh": "^0.8.3",
    "style-loader": "^1.3.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-beta.3"
  },
  "optionalDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/supertest": "^2.0.11",
    "@types/winston": "^2.4.4"
  }
}
